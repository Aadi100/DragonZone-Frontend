@page
@model POS.Pages.addorganizationModel
@{
}



<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0">
    <meta name="description" content="POS - Bootstrap Admin Template">
    <meta name="keywords" content="admin, estimates, bootstrap, business, corporate, creative, invoice, html5, responsive, Projects">
    <meta name="author" content="Dreamguys - Bootstrap Admin Template">
    <meta name="robots" content="noindex, nofollow">
    <title>Dreams Pos admin template</title>

    <link rel="shortcut icon" type="image/x-icon" href="assets/img/favicon.jpg">

    <link rel="stylesheet" href="assets/css/bootstrap.min.css">

    <link rel="stylesheet" href="assets/css/animate.css">

    <link rel="stylesheet" href="assets/plugins/select2/css/select2.min.css">

    <link rel="stylesheet" href="assets/css/dataTables.bootstrap4.min.css">

    <link rel="stylesheet" href="assets/plugins/fontawesome/css/fontawesome.min.css">
    <link rel="stylesheet" href="assets/plugins/fontawesome/css/all.min.css">

    <link rel="stylesheet" href="assets/css/style.css">
</head>
<body>
    <div id="global-loader">
        <div class="whirly-loader"> </div>
    </div>

    <div class="main-wrapper">
        <div class="page-wrapper">
            <div class="content">
                <div class="page-header">
                    <div class="page-title">
                        <h4>Organization Management</h4>
                        <h6>Add/Update Organization</h6>
                    </div>
                </div>

                <div class="card">
                    <div class="card-body">
                        <form method="post">
                            <div class="row">
                                <div class="col-lg-3 col-sm-6 col-12">
                                    <div class="form-group">
                                        <label asp-for="ORG.Name"></label>
                                        <input asp-for="ORG.Name" class="form-control" />
                                        <span asp-validation-for="ORG.Name" class="text-danger"></span>
                                    </div>
                                    <div class="form-group">
                                        <label asp-for="ORG.Address"></label>
                                        <input asp-for="ORG.Address" class="form-control" />
                                        <span asp-validation-for="ORG.Address" class="text-danger"></span>
                                    </div>
                                </div>
                                <div class="col-lg-3 col-sm-6 col-12">
                                    <div class="form-group">
                                        <label asp-for="ORG.Country"></label>
                                        <input asp-for="ORG.Country" class="form-control" />
                                        <span asp-validation-for="ORG.Country" class="text-danger"></span>
                                    </div>
                                    <div class="form-group">
                                        <label asp-for="ORG.CP_Name"></label>
                                        <input asp-for="ORG.CP_Name" class="form-control" />
                                        <span asp-validation-for="ORG.CP_Name" class="text-danger"></span>
                                    </div>
                                </div>
                                <div class="col-lg-3 col-sm-6 col-12">
                                    <div class="form-group">
                                        <label asp-for="ORG.CP_Phone_Number"></label>
                                        <input asp-for="ORG.CP_Phone_Number" class="form-control" />
                                        <span asp-validation-for="ORG.CP_Phone_Number" class="text-danger"></span>
                                    </div>
                                    <div class="form-group">
                                        <label asp-for="ORG.NTN"></label>
                                        <input asp-for="ORG.NTN" class="form-control" />
                                        <span asp-validation-for="ORG.NTN" class="text-danger"></span>
                                    </div>
                                </div>
                                <div class="col-lg-3 col-sm-6 col-12">
                                    <div class="form-group">
                                        <label asp-for="ORG.City"></label>
                                        <input asp-for="ORG.City" class="form-control" />
                                        <span asp-validation-for="ORG.City" class="text-danger"></span>
                                    </div>
                                    <div class="form-group">
                                        <label asp-for="ORG.CP_Email_Address"></label>
                                        <input asp-for="ORG.CP_Email_Address" class="form-control" />
                                        <span asp-validation-for="ORG.CP_Email_Address" class="text-danger"></span>
                                    </div>
                                </div>
                                <div class="col-lg-3 col-sm-6 col-12">
                                    <div class="form-group">
                                        <label asp-for="ORG.Percentage"></label>
                                        <input asp-for="ORG.Percentage" class="form-control" />
                                        <span asp-validation-for="ORG.Percentage" class="text-danger"></span>
                                    </div>
                                </div>
                                <div class="col-lg-12">
                                    <button type="submit" class="btn btn-submit me-2">Submit</button>
                                    <a asp-page="/productlist" class="btn btn-cancel">Cancel</a>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>

            </div>
        </div>
    </div>


    <script src="assets/js/jquery-3.6.0.min.js"></script>

    <script src="assets/js/feather.min.js"></script>

    <script src="assets/js/jquery.slimscroll.min.js"></script>

    <script src="assets/js/jquery.dataTables.min.js"></script>
    <script src="assets/js/dataTables.bootstrap4.min.js"></script>

    <script src="assets/js/bootstrap.bundle.min.js"></script>

    <script src="assets/plugins/select2/js/select2.min.js"></script>

    <script src="assets/plugins/sweetalert/sweetalert2.all.min.js"></script>
    <script src="assets/plugins/sweetalert/sweetalerts.min.js"></script>

    <script src="assets/js/script.js"></script>
</body>